{
  "name": "gateway-database",
  "private": true,
  "version": "2.1.0",
  "description": "database connection to external",
  "main": "main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "npm run webpack && electron .",
    "start-dev": "set ELECTRON_DEVELOP=1 && npm run webpack && electron .",
    "start-server": "node ./server/index.js",
    "webpack": "webpack --mode production",
    "webpack-w": "webpack --mode development -w",
    "build": "npm run clean && npm run webpack && build -p never",
    "release": "npm run clean && npm run webpack && build -p always",
    "clean": "npm run clean-build && npm run clean-webpack",
    "clean-webpack": "rm -rf ./dist",
    "clean-build": "rm -rf ./build",
    "test": "jest",
    "lint": "eslint ."
  },
  "keywords": [
    "electron",
    "react",
    "sqlite3",
    "postgresql",
    "mysql",
    "mssql",
    "socket.io",
    "request",
    "bootstrap"
  ],
  "author": "Anachitheu.k@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anechasun/gateway-database.git"
  },
  "homepage": "https://github.com/Anechasun/gateway-database.git#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.10.2",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "mssql": "^5.1.0",
    "mysql": "^2.17.1",
    "pg": "^7.4.3",
    "popper.js": "^1.14.4",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "request": "^2.88.0",
    "socket.io": "^2.2.0",
    "sqlite3": "^4.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^24.9.0",
    "babel-loader": "^7.1.5",
    "css-loader": "^1.0.0",
    "electron": "^9.1.0",
    "electron-builder": "^20.26.1",
    "electron-rebuild": "^1.8.2",
    "eslint": "^5.3.0",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.17.1",
    "file-loader": "^1.1.11",
    "jest": "^24.9.0",
    "prettier": "^1.14.0",
    "react-test-renderer": "^16.9.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "build": {
    "appId": "com.electron.gateway-database",
    "productName": "gateway-database",
    "directories": {
      "output": "build/"
    },
    "files": [
      "database",
      "node_modules/",
      "dist/",
      "renderer/",
      "resources/",
      "utils/",
      "Memory.js",
      "main.js",
      "config.js",
      "package.json"
    ],
    "win": {
      "icon": "resources/icon.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Anechasun",
        "repo": "gateway-database"
      }
    ]
  }
}
